{
    "name": "stuck-overflow",
    "version": "1.0.0",
    "description": "",
    "main": "dist/app.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "prebuild": "npm run clean",
        "clean": "rimraf dist",
        "dev": "concurrently \"tsc -w\" \"nodemon -r dotenv/config dist/app.js -w dist\"",
        "predev": "npm run build",
        "start": "node dist/app.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "docker-build": "docker build -t stuck-overflow-api:latest .",
        "docker-run": "docker run --rm -p 3000:3000 stuck-overflow-api:latest",
        "docker-clean": "docker rmi stuck-overflow-api:latest",
        "migrate": "node -r dotenv/config tools/run-migrations.js",
        "premigrate": "npm run build",
        "seed-db": "node -r dotenv/config tools/seed-database.js",
        "preseed-db": "npm run build"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/debug": "^4.1.8",
        "@types/express": "^4.17.17",
        "@types/morgan": "^1.9.4",
        "@types/node": "^20.4.2",
        "@types/pg": "^8.10.2",
        "concurrently": "^8.2.0",
        "nodemon": "^3.0.1",
        "postgres-migrations": "^5.3.0",
        "rimraf": "^5.0.1",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "debug": "^4.3.4",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.0.0",
        "morgan": "^1.10.0",
        "pg": "^8.11.1"
    }
}
